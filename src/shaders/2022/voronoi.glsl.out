uniform float f;uniform vec2 r;vec2 v(vec2 v){return v=vec2(dot(v,vec2(127.1,311.7)),dot(v,vec2(269.5,183.3))),fract(sin(v)*18.5453);}vec2 t(in vec2 f){vec2 i=floor(f),s=fract(f);vec3 d=vec3(8.);for(int r=-1;r<=1;r++)for(int y=-1;y<=1;y++){vec2 t=vec2(float(y),float(r)),x=v(i+t),c=t-s+(.5+.5*sin(s+6.2831*x));float m=dot(c,c);if(m<d.x)d=vec3(m,x);}return vec2(sqrt(d.x),d.y+d.z);}void main(){vec2 v=gl_FragCoord.xy/r,d=t(20.*v);vec3 i=.5+.5*abs(cos(f+d.y+vec3(1.,0.,.4)));gl_FragColor=vec4(i,1.);}